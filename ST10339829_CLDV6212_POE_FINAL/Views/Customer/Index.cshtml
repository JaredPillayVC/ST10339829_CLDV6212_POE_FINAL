@model List<Customer>

<h2>Customer List</h2>

<table class="table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Phone</th>
            <th>Email</th>
            <th>Address</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var customer in Model)
        {
            <tr>
                <td>@customer.Name</td>
                <td>@customer.Phone</td>
                <td>@customer.Email</td>
                <td>@customer.Address</td>
                <td>
                    <a href="@Url.Action("Edit", new { id = customer.RowKey })" class="btn btn-primary">Edit</a>
                    <a href="@Url.Action("Delete", new { id = customer.RowKey })" class="btn btn-danger">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

<a href="@Url.Action("Create")" class="btn btn-success">Create New Customer</a>
